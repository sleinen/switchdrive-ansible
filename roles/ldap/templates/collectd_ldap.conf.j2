<LoadPlugin perl>
    Globals true
</LoadPlugin>

<Plugin perl>
    IncludeDir "/opt/collectd/collectd.plugins/"
    BaseName ""
    #EnableDebugger ""
    
    LoadPlugin "LDAP"
    <Plugin LDAP>
      IP "127.0.0.1"
      PORT "389"
      USER "cn=monitor"
      PASSWORD "{{ldap_cn_config_password}}"
    </Plugin>
</Plugin>
